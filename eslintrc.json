{
  "extends": ["standard"],
  "parser": "babel-eslint",
  "plugins": ["ember-cleanup", "mocha"],
  "rules": {
    "camelcase": 2,
    "complexity": [2, 5],
    "ember-cleanup/cp-macro-alias": 2,
    "ember-cleanup/destructuring": 2,
    "ember-cleanup/no-console": 2,
    "ember-cleanup/no-dup-keys": 2,
    "ember-cleanup/no-expr-in-dep-keys": 2,
    "ember-cleanup/no-is-array": 2,
    "ember-cleanup/no-set-in-getter": 2,
    "ember-cleanup/no-settimeout": 2,
    "ember-cleanup/no-this-in-dep-keys": 2,
    "ember-cleanup/no-throw": 2,
    "ember-cleanup/no-typeof": 2,
    "ember-cleanup/one-level-each": 2,
    "ember-cleanup/route-model-return": 2,
    "ember-cleanup/square-brackets": 2,
    "ember-cleanup/super-args": 2,
    "max-len": [2, 120, 4, {
      "ignoreComments": true,
      "ignoreUrls": true
    }],
    "mocha/handle-done-callback": 2,
    "mocha/no-exclusive-tests": [2, {
      "additionalTestFunctions": [
        "describeComponent",
        "describeModel",
        "describeModule"
      ]
    }],
    "mocha/no-global-tests": 2,
    "mocha/no-pending-tests": 2,
    "mocha/no-skipped-tests": [1, {
      "additionalTestFunctions": [
        "describeComponent",
        "describeModel",
        "describeModule"
      ]
    }],
    "no-unused-expressions": [2, {
      "allowTernary": true
    }],
    "valid-jsdoc": [
      2,
      {
        "prefer": {
          "virtual": "abstract",
          "extends": "augments",
          "constructor": "class",
          "const": "constant",
          "defaultvalue": "default",
          "desc": "description",
          "host": "external",
          "fileoverview": "file",
          "overview": "file",
          "emits": "fires",
          "func": "function",
          "method": "function",
          "var": "member",
          "arg": "param",
          "argument": "param",
          "return": "returns",
          "exception": "throws"
        },
        "requireReturn": false
      }
    ]
  }
}
